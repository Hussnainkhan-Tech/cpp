#include <iostream>
using namespace std;
int main() {
int size = 4;
for (int i = 1; i <= size; ++i) {
for (int j = 1; j <= size; ++j) {
if (i == j) {
cout << i;
} else {
cout << i * 2;
}}
cout << endl;
}
return 0;
}
Program no 8
Print the following shape
#####*#####
####*#*####
###*###*###
##*#####*##
#*#######*#
*#########*
#include <iostream>
using namespace std;
int main() {
int size = 6;
for (int i = 0; i < size; ++i) {
for (int j = 0; j < size * 2 + 1; ++j) {
if (j == size - i || j == size + i) {
cout << '*';
} else {
cout << '#';
}}
cout << endl;
}
return 0;
}
Program no 9
Print the shape 
BBBBBBBBB
.BBBBBBB 
..BBBBB 
...BBB 
.....B
#include <iostream>
using namespace std;
int main() {
int rows = 5;
for (int i = 0; i < rows; ++i) {
for (int j = 0; j < i; ++j) {
cout << '.';
}
for (int k = 0; k < (2 * rows - 1 - 2 * i); ++k) {
cout << 'B';
}
cout << endl;
}
return 0;
}

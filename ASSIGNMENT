Program no 18 
Print the following shape
_____________
|  --------- |
| --------   |
|  --------- |
| ---------  |
Â¬|____________|

#include <iostream>
using namespace std;
int main() {
    int height = 6;
    int width = 14;
// Top 
    for (int i = 0; i < width; ++i) {
    cout << '_';
    }
    cout << endl;
// Upper 
    cout << '|';
    cout << "  --------- ";  // Specific alignment
    cout << '|' << endl;

    cout << '|';
    cout << " --------   ";  // Specific alignment
    cout << '|' << endl;
    cout << '|';
    cout << "  --------- ";  // Specific alignment
    cout << '|' << endl;
    cout << '|';
    cout << " ---------  ";  // Specific alignment
    cout << '|' << endl;
// Bottom
    cout << '|';
    for (int i = 1; i < width - 1; ++i) {
        cout << '_';
    }
    cout << '|' << endl;
    return 0;
    }
Program no 19
Print the following shape
ABCDEF
BCDEFA
CDEFAB
DEFABC
EFABCD
FABCDE
#include <iostream>
using namespace std;
int main() {
    int rows = 6;
    int cols = 6;
for (int i = 0; i < rows; ++i) {
    for (int j = 0; j < cols; ++j) {
    cout << char('A' + (i + j) % cols);
    }
    cout << endl;
    }
    return 0;
    }
Program no 20
Print the following shape
1   1
 2 2
  3
 2 2
1   1
#include <iostream>
using namespace std;
int main() {
    int rows;
    cout << "Enter the number of rows: ";
    cin >> rows;
// Upper
    for (int i = 1; i < rows; ++i) {
        for (int j = 1; j < i; ++j) {
            cout << " ";
        }
        cout << i;
        for (int j = 1; j < 2 * (rows - i); ++j) {
            cout << " ";
        }
        cout << i << endl;
    }

    // Middle 
    for (int i = 1; i < rows; ++i) {
        cout << " ";
    }
    cout << rows << endl;

    // Lower 
    for (int i = rows - 1; i >= 1; --i) {
        for (int j = 1; j < i; ++j) {
            cout << " ";
        }
        cout << i;
        for (int j = 1; j < 2 * (rows - i); ++j) {
            cout << " ";
        }
        cout << i << endl;
    }

    return 0;
}
Program no 21
Print the following shape
***1***
**121**
*12321*
#include <iostream>

using namespace std;

int main() {
    int rows = 3;

    for (int i = 1; i <= rows; ++i) {
        // Print leading stars
        for (int j = rows; j > i; --j) {
            cout << '*';
        }
        // ascending 
        for (int k = 1; k <= i; ++k) {
            cout << k;
        }
        // descending numbers
        for (int l = i - 1; l >= 1; --l) {
            cout << l;
        }
        // Print trailing stars
        for (int m = rows; m > i; --m) {
            cout << '*';
        }
        cout << endl;
        }

        return 0;
        }

Program no 22
Print the following shape
    5      
   4 4     
  3   3    
 2     2   
1       1  
#include <iostream>
using namespace std;
int main() {
    int rows = 5;
for (int i = 0; i < rows; ++i) {
        for (int j = rows - i; j > 0; --j) {
            cout << ' ';
            }
        cout << rows - i;
        if (i > 0) {
            for (int j = 1; j < 2 * i; ++j) {
            cout << ' ';
            }
            cout << rows - i;
            }
        cout << endl;
            }

    return 0;
            }
Program no 23
1357
135 
13  
1    
13   
135  
1357  
#include <iostream>
using namespace std;
int main() {
    int rows = 7;
    int max_numbers = 4;
	for (int i = 0; i < rows; ++i) {
        int limit = (i < 4) ? (max_numbers - i) : (i - 2);
        for (int j = 0; j < limit; ++j) {
        cout << (1 + 2 * j);
        }
        cout << endl;
        }
        return 0;
        }




	
Program no 24
Print the following shape
1  4  16  36  64
1  9  25  49    
1  4  16        
1  9              
1                 
#include <iostream>
#include <iomanip>
using namespace std;
int main() {
    int rows = 5;
   
 int num = 1;
 for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < rows - i; ++j) {
            cout << setw(4) << num * num;
            num += 1 + (num % 2);  // Ensure num is alternately odd and even
           }
            cout << endl;
           }
          return 0;
           }
Program no 25
A C E G
A C E
A C
A
#include <iostream>
using namespace std;
int main() {
    int rows = 4;
    char startChar = 'A';
for (int i = 0; i < rows; ++i) {
        char currentChar = startChar;
        for (int j = 0; j < rows - i; ++j) {
            cout << currentChar << ' ';
            currentChar += 2;  
            }
            cout << endl;
            }    
            return 0;
            }












PROGRAM NO 26
Print the following shape
*
*1*
*121*
*12321*
*1234321*
*123454321*
*1234321*
*12321*
*121*
*1*
*
#include <iostream>
using namespace std;
int main() {
    int rows = 6;
 // upper half 
    for (int i = 1; i <= rows; ++i) {
        cout << '*';
        for (int j = 1; j <= i; ++j) {
        cout << j;
        }
        for (int j = i - 1; j >= 1; --j) {
        cout << j;
        }
        cout << '*' << endl;
        }
 // lower hal
    for (int i = rows - 1; i >= 1; --i) {
        cout << '*';
        for (int j = 1; j <= i; ++j) {
        cout << j;
        }
        for (int j = i - 1; j >= 1; --j) {
        cout << j;
        }
        cout << '*' << endl;
        }
        cout << '*' << endl;
        return 0;
        }
Program no 27
Print the following shape
*****
 *  *
  * *
   **
    *
#include <iostream>
using namespace std;
int main() {
    int rows = 5;
for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < i; ++j) {
        cout << ' ';
        }
        for (int k = 0; k < (rows - i); ++k) {
        if (i == 0 || k == 0 || k == rows - i - 1) {
        cout << '*';
        } else {
        cout << ' ';
        }
        }
        cout << endl;
        }

       return 0;
         }  


Program no 28
Print the following shape

* 
**
* *
*  *
*****
#include <iostream>
using namespace std;
int main() {
    int rows = 5;
for (int i = 0; i < rows; ++i) {
        for (int j = 0; j <= i; ++j) {
            if (i == rows - 1 || j == 0 || j == i) {
             cout << '*';
            } else {
             cout << ' ';
            }  }
             cout << endl;
            }

            return 0;
            }
Program no 29
Print the following shape
    *****   
   *   *   
  *   *      
 *   *    
*****   
#include <iostream>
using namespace std;
int main() {
    int rows = 5;
    int cols = 5;
for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < cols; ++j) {
        if (i == 0 || i == rows - 1 || j == cols - i - 1 || j == cols - 1) {
                cout << '*';
            } else {
                cout << ' ';
            }
            }
                cout << endl;
            }  
               return 0;
            }
       

Program no 11
Print the shape 
    *
   ***
  *****
 *******
*********
 *******
  *****
   ***
    *
#include <iostream>
using namespace std;
int main() {
int rows = 5;
// upper half 
for (int i = 1; i <= rows; ++i) {
// spaces
for (int j = i; j < rows; ++j) {
cout << ' ';
}
// Print * 
for (int k = 1; k <= (2 * i - 1); ++k) {
cout << '*';
}
cout << endl;
}
// lower half 
for (int i = rows - 1; i >= 1; --i) {
// Print spaces
for (int j = rows; j > i; --j) {
cout << ' ';
}
// Print *
for (int k = 1; k <= (2 * i - 1); ++k) {
cout << '*';
}
cout << endl;
}
Return 0;
}
Program no 12
Print following shape
         1
        232
       34543
      4567654
     567898765
    67890109876
   7890123210987
  890123454321098
 90123456765432109
0123456789876543210




#include <iostream>
using namespace std;
int main() {
int rows = 10;
for (int i = 0; i < rows; ++i) {
// Print leading spaces
for (int j = rows - 1; j > i; --j) {
cout << ' ';
}
for (int k = 0; k <= i; ++k) {
cout << (i + k) % 10;
}
for (int l = i - 1; l >= 0; --l) {
cout << (i + l) % 10;
}
cout << endl;
}
return 0;
}
Program no 13

Program no 1  
print the following shape
* * * * *
*          *
*          *
*          *
* * * * *
#include <iostream>
using namespace std;
int main() {
int rows = 5;
int cols = 5;
for (int i = 0; i < rows; ++i) {
for (int j = 0; j < cols; ++j) {
if (i == 0 || i == rows - 1) {
cout << "* ";                
} else {
if (j == 0 || j == cols - 1) {
cout << "* ";
} else {
cout << "  ";
}
}
}
cout << endl;
}
return 0
program no 2
    1
   12
  123
 1234
12345
#include <iostream>
using namespace std;
int main() {
int rows = 5;
for (int i = 1; i <= rows; ++i) {
for (int j = rows; j > i; --j) {
cout << "  ";
}
for (int k = 1; k <= i; ++k) {
cout << k << " ";
}
cout << endl;
}
Return 0;

{
    case 35:
    cout<<"Name--> MASROOR ALI"<<endl;
    cout<<"Location--> SATALIGHT TOWN"<<endl;
    cout<<"Phone--> +92111111111"<<endl;
    break;
    case 7:
    cout<<"Name--> WASIF SAHFIQ"<<endl;
    cout<<"Location--> MASOOM SHAH COLONY"<<endl;
    cout<<"Phone--> +92111111111"<<endl;
    break;
    case 47:
    cout<<"Name--> HASNIAN KHAN"<<endl;
    cout<<"Location--> HABIB COLONY"<<endl;
    cout<<"Phone--> +92111111111"<<endl;
    break;
    case 1:
    cout<<"Name--> WASEEM ABBAS"<<endl;
    cout<<"Location--> 413/EB"<<endl;
    cout<<"Phone--> +92111111111"<<endl;
    break;
    case 10:
    cout<<"Name--> GHULAM ALI"<<endl;
    cout<<"Location--> MARZI PURA"<<endl;
    cout<<"Phone--> +92111111111"<<endl;
    break;
    default:
    cout<<"DATA TO PURA DEDE GANDU";
}

{OPRETOR}............................................


int a,b;
char ch;
cout<<"enter the first number";
cin>>a;
cout<<"enter the second number";
cin>>b;
cout<<"enter the character";
cin>>ch;
switch(ch)
{
    case '+':
    cout<<"sum of the number"<<a+b;
    break;
    case '-':
    cout<<"subtract of the number"<<a-b;
    break;
    case '*':
    cout<<"multiply the number"<<a*b;
    break;
    case '/':
    cout<<"divide the number"<<a/b;
    break;
    default:
    cout<<"BSDK SAHI VALUE DE";

}




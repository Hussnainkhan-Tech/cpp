cin>>choice;

    switch(choice)
    {
        case 1:
        int a;
        
        cout<<"enter the roll number";
        cin>>a;
        if(a==7 || a==35 || a==47 || a==2 || a==9)
        {
        if(a==7){
        cout<<"NAME---> WASIF SAHFIQ"<<endl;

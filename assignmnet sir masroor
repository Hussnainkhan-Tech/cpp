long int accNo;
int pass;
int balance=10000;
cout<<"enter the Account number";
cin>>accNo;
cout<<"enter the Password";
cin>>pass;
if(accNo==123456789 && pass==1122)
{
    cout<<"successfully login!";
    cout<<endl<<"1. cash deposit"<<endl;
    cout<<"2. withdraw"<<endl;
    cout<<"3. check balance"<<endl;
    cout<<"4. send money"<<endl;

int choice;
cout<<"Select the number";
cin>>choice;
switch(choice)
{
    case 1:
int amount;
cout<<"enter the Amount";
cin>>amount;
amount=amount+balance;
cout<<"congratulation! YOUR AMOUNT IS ::"<<amount;
break;
case 2:
cout<<"enter the amount";
cin>>amount;
if(amount<=balance){
cout<<"OKAY!";
amount=amount-balance;
cout<<"withdrawal Successfully!"<<endl<<"Now your balance is ::"<<amount;}
else
cout<<"insufficiant balance recharged your Acount";


break;
case 3:
cout<<"your balance is ::"<<balance;
break;
case 4:
{
cout<<"enter the account number";
cin>>accNo;
cout<<"enter the amount";
cin>>amount;
}
if(amount>0 && amount<=balance){
cout<<"MONEY SENT!"<<endl;
balance=balance-amount;
cout<<"your currently balance is::"<<balance;}
else
cout<<"insufficiant balance";
break;
default:
cout<<"agla number tere baap ne likha ha sally!";
}
}else
{
cout<<"invalid password";
}


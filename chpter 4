    return 0;
}


#include<iostream>
using namespace std;
int main()
{
    float base,height;
    cout<<"enter the base";
    cin>>base;
    cout<<"enter the height";
    cin>>height;
    float area=(base*height)/2;
    cout<<"area of triangle ::"<<area;
    return 0;
}


#include<iostream>
using namespace std;
int main()
{
int h,m,s;
cout<<"enter the time in seconds";
cin>>s;
h=s/3600;
s=s%3600;
m=s/60;
s=s%60;
cout<<"HH-MM-SEC="<<h<<":"<<m<<":"<<s<<endl;
return 0;
}



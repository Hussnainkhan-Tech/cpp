#include<iostream>
using namespace std;
int main()
{
    float a,c,r;
    cout<<"entr the radius ::";
    cin>>r;
   float area=r*r*3.14;
   float circum=2*r*3.14;
    cout<<"area is ::"<<area<<endl;
    cout<<"circum is ::"<<circum<<endl;
    return 0;
}


#include <iostream>
using namespace std;
int main()
{
    int vf,vi,a,t;
    cout<<"enter the initial velocity::"<<endl;
    cin>>vi;
    cout<<"acceleration"<<endl;
    cin>>a;
    cout<<"enter the time::"<<endl;
    cin>>t;
    vf=vi+a*t;
    cout<<"the final velocity::"<<vf;
    return 0;

}


#include <iostream>
using namespace std;
int main()
{
    int n,a,b;
    cout<<"enter the value";
    cin>>n;
    a=n/100;
    n=n%100;
    b=n/10;
    n=n%10;
    cout<<"the reverse order ::"<<n<<endl<<b<<endl<<a<<endl;
    return 0;
}


#include <iostream>
using namespace std;
int main()
{
int e,o,result;
cout<<"enter the even number";
cin>>e;
cout<<"enter the odd number";
cin>>o;
result=1000-((e*5)+(o*3));
cout<<"result is ::"<<result;
return 0;
}


#include <iostream>
using namespace std;
int main()
{
int hrs,d,w;
cout<<"entr the hours";
cin>>hrs;
w=hrs/168;
hrs=hrs%168;
d=hrs/24;
hrs=hrs/24;
cout<<"weeks ::"<<w<<":"<<"days"<<d<<":"<<endl<<"hours"<<hrs;
return 0;
}


#include <iostream>
using namespace std;
int main()
{
    float e1,e2,g1,g2,p1,p2;
    cout<<"old price of electricity::";
    cin>>e1;
    cout<<"old price of gas::";
    cin>>g1;
    cout<<"old price of petrol::";
    cin>>p1;
    e2=e1*2.2;
    g2=g1*2.2;
    p2=p1*2.2;
    cout<<"New price of electricity::"<<e2<<endl;
    cout<<"New price of gas::"<<g2<<endl;
    cout<<"New price of petrol::"<<p2<<endl;
    return 0;
}


#include<iostream>
using namespace std;
int main()
{
    cout<<"C"<<endl;
    cout<<"O M"<<endl;
